.center-container
  .absolute-center.text-center
    %h1.text-center Add Experts
    = form_tag users_path, :method => 'get' do
      .input-field
        = text_field_tag :search, params[:search]
      .action-button.submit
        = submit_tag "Search", :name => nil
    %ul.pull-left
      - @users.each do |user|
        %li= user.username
